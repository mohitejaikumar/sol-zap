{
  "name": "sol-zap",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start:frontend":"cd apps/frontend && yarn run start",
    "start:backend":"cd apps/primary-backend && yarn run start",
    "start:worker":"cd apps/worker && yarn run start",
    "start:processor":"cd apps/processor && yarn run start",
    "start:hookbackend":"cd apps/hookBackend && yarn run start"
    
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.14",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
